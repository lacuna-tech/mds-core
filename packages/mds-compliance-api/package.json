{
  "name": "@lacuna-core/lacuna-compliance-api",
  "description": "Lacuna Compliance API",
  "version": "0.1.0",
  "author": "Lacuna Technologies",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "PATH_PREFIX=/compliance yarn watch server",
    "test": "yarn test:eslint && yarn test:unit",
    "test:eslint": "eslint --ignore-path ../../.gitignore '**/*.ts'",
    "test:unit": "PATH_PREFIX=/compliance DOTENV_CONFIG_PATH=../../.env jest",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env node --experimental-repl-await -r ts-node/register -r dotenv/config",
    "watch": "yarn build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec yarn ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-helpers": "0.1.27-lacuna-tech.316",
    "@mds-core/mds-api-server": "0.1.27-lacuna-tech.316",
    "@mds-core/mds-logger": "0.1.25-lacuna-tech.316",
    "@mds-core/mds-service-helpers": "0.1.1-lacuna-tech.816",
    "@mds-core/mds-types": "0.1.24-lacuna-tech.316",
    "@mds-core/mds-utils": "0.1.27-lacuna-tech.316",
    "@lacuna-core/lacuna-compliance-service": "0.1.0",
    "@types/express": "4.17.8",
    "express": "4.17.1",
    "http-status-codes": "2.1.4",
    "joi": "17.3.0"
  }
}
