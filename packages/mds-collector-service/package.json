{
  "name": "@mds-core/mds-collector-service",
  "version": "0.1.0",
  "description": "Mobility Data Specification Collector Service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "keywords": [
    "mds"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "yarn watch server",
    "test": "yarn test:eslint && yarn test:unit",
    "test:eslint": "eslint --ignore-path ../../.gitignore '**/*.ts'",
    "test:unit": "DOTENV_CONFIG_PATH=../../.env jest",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env node --experimental-repl-await -r ts-node/register -r dotenv/config",
    "watch": "yarn build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec yarn ts-node --"
  },
  "author": "City of Los Angeles",
  "license": "Apache-2.0",
  "dependencies": {
    "ajv": "7.0.3",
    "ajv-formats": "1.5.1"
  }
}
