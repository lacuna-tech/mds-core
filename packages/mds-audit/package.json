{
  "name": "@mds-core/mds-audit",
  "version": "0.1.38",
  "description": "Endpoint for auditing compliance with MDS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "PATH_PREFIX=/audit pnpm watch server",
    "test": "pnpm test:eslint && pnpm test:unit",
    "test:eslint": "eslint --ignore-path ../../.gitignore '**/*.ts'",
    "test:unit": "PATH_PREFIX=/audit DOTENV_CONFIG_PATH=../../.env nyc --use-spawn-wrap ts-mocha --project ../../tsconfig.json",
    "ts-node": "pnpm build && DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "keywords": [
    "mds",
    "audit"
  ],
  "author": "City of Los Angeles",
  "license": "Apache-2.0",
  "dependencies": {
    "@mds-core/mds-agency-cache": "workspace:0.1.26",
    "@mds-core/mds-api-helpers": "workspace:0.1.26",
    "@mds-core/mds-api-server": "workspace:0.1.26",
    "@mds-core/mds-attachment-service": "workspace:0.1.0",
    "@mds-core/mds-db": "workspace:0.1.26",
    "@mds-core/mds-logger": "workspace:0.1.24",
    "@mds-core/mds-providers": "workspace:0.1.26",
    "@mds-core/mds-schema-validators": "workspace:0.1.2",
    "@mds-core/mds-service-helpers": "workspace:0.1.0",
    "@mds-core/mds-types": "workspace:0.1.23",
    "@mds-core/mds-utils": "workspace:0.1.26",
    "@types/express": "4.17.10",
    "@types/multer": "1.4.5",
    "@types/sharp": "0.27.1",
    "aws-sdk": "2.826.0",
    "express": "4.17.1",
    "multer": "1.4.2",
    "sharp": "0.27.0"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "workspace:0.1.26"
  }
}
