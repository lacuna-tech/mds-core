{
  "name": "@container-images/mds-web-sockets",
  "version": "0.0.1",
  "description": "Docker Image for MDS Web Sockets",
  "private": true,
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "bundle": "webpack --mode=production --env.npm_package_name=${npm_package_name} --env.npm_package_version=${npm_package_version}",
    "image": "yarn bundle && ../../bin/build-helper buildImage mds-web-sockets:${npm_package_version}",
    "test": "yarn test:eslint",
    "test:eslint": "eslint --ignore-path ../../.gitignore '**/*.ts'"
  },
  "keywords": [
    "mds"
  ],
  "author": "City of Los Angeles",
  "license": "Apache-2.0",
  "dependencies": {
    "@mds-core/mds-web-sockets": "0.0.1",
    "@mds-core/mds-webpack-config": "0.1.0"
  }
}
