{
  "name": "@container-images/mds-daily",
  "version": "0.1.26",
  "private": true,
  "description": "MDS daily-reports prototype",
  "keywords": [
    "mds"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "bundle": "pnpm bundle:production",
    "bundle:development": "webpack --mode=development --env npm_package_name=${npm_package_name} --env npm_package_version=${npm_package_version}",
    "bundle:production": "webpack --mode=production --env npm_package_name=${npm_package_name} --env npm_package_version=${npm_package_version}",
    "image": "pnpm image:production",
    "image:development": "pnpm bundle:development && ../../bin/build-helper buildImage mds-daily:${npm_package_version}",
    "image:production": "pnpm bundle:production && ../../bin/build-helper buildImage mds-daily:${npm_package_version}",
    "test": "pnpm test:eslint",
    "test:eslint": "eslint --ignore-path ../../.gitignore '**/*.ts'"
  },
  "dependencies": {
    "@mds-core/mds-daily": "0.0.27",
    "@mds-core/mds-webpack-config": "0.1.0"
  }
}
