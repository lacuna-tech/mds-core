{
  "name": "@container-images/mds-transactions-service",
  "version": "0.0.1",
  "description": "Docker Image for MDS Transactions Service",
  "private": true,
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "bundle": "webpack --mode=production --env npm_package_name=${npm_package_name} --env npm_package_version=${npm_package_version}",
    "image": "pnpm bundle && ../../bin/build-helper buildImage mds-transactions-service:${npm_package_version}",
    "test": "pnpm test:eslint",
    "test:eslint": "eslint --ignore-path ../../.gitignore '**/*.ts'"
  },
  "keywords": [
    "mds"
  ],
  "author": "City of Los Angeles",
  "license": "Apache-2.0",
  "dependencies": {
    "@mds-core/mds-transactions-service": "workspace:0.1.0",
    "@mds-core/mds-webpack-config": "workspace:0.1.0"
  }
}
