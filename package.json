{
  "name": "root",
  "version": "0.1.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "scripts": {
    "build": "tsc --version && pnpm run:concurrent build",
    "bundle": "webpack --version && pnpm run:concurrent bundle",
    "bundle:development": "pnpm run:concurrent bundle:development",
    "bundle:production": "pnpm run:concurrent bundle:production",
    "clean": "git clean -fdX -e '!.env' -e '!helm/mds/charts' -e '!helm/mds/charts/**' -e '!helm/mds/files' -e '!helm/mds/files/**' && pnpm install",
    "generate": "pnpm run:concurrent generate",
    "image": "pnpm run:concurrent image",
    "image:development": "pnpm run:concurrent image:development",
    "image:production": "pnpm run:concurrent image:production",
    "images": "pnpm run:concurrent image",
    "preinstall": "npx check-node-version --package && npx only-allow pnpm && pnpm i -g pnpm",
    "lint": "pnpm lint:audit && pnpm lint:prettier '**/package.json' && pnpm lint:eslint '**/!(*.spec).ts'",
    "lint:audit": "pnpm-ci audit -i 1556,1561,1753 --strict",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-path .eslintignore",
    "lint:prettier": "prettier --check",
    "prepare": "husky install",
    "push": "bin/build-helper pushImages",
    "run:concurrent": "pnpm recursive run --filter=!root",
    "run:sequential": "pnpm recursive run --workspace-concurrency=1 --filter=!root",
    "test": "pnpm run:sequential test",
    "values": "bin/build-helper templateValues"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@types/jest": "26.0.23",
    "@types/mocha": "8.2.2",
    "@types/node": "15.12.2",
    "@types/sinon": "10.0.2",
    "@types/sinon-express-mock": "1.3.9",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "dotenv": "10.0.0",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-recommended": "4.1.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-log": "1.2.7",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-progress": "0.0.1",
    "eslint-plugin-promise": "5.1.0",
    "git-revision-webpack-plugin": "5.0.0",
    "husky": "6.0.0",
    "jest": "27.0.4",
    "lerna": "4.0.0",
    "lint-staged": "11.0.0",
    "mocha": "9.0.0",
    "mochawesome": "6.2.2",
    "mockdate": "3.0.5",
    "node-rsa": "1.1.1",
    "nodemon": "2.0.7",
    "nyc": "15.1.0",
    "pnpm-ci-tools": "1.0.1",
    "prettier": "2.3.1",
    "prettier-plugin-packagejson": "2.2.11",
    "should": "13.2.3",
    "sinon": "11.1.1",
    "sinon-express-mock": "2.2.1",
    "source-map-support": "0.5.19",
    "supertest": "6.1.3",
    "ts-jest": "27.0.3",
    "ts-loader": "9.2.3",
    "ts-mocha": "8.0.0",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.9.0",
    "typedoc": "0.20.36",
    "typedoc-plugin-lerna-packages": "0.3.1",
    "typescript": "4.2.4",
    "unit.js": "2.1.1",
    "webpack": "5.38.1",
    "webpack-cli": "4.7.2",
    "webpack-merge": "5.8.0",
    "wtfnode": "0.8.4"
  },
  "engines": {
    "node": "15.11.0"
  }
}
