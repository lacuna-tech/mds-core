{
  "name": "root",
  "private": true,
  "version": "0.0.20",
  "license": "Apache-2.0",
  "workspaces": [
    "container-images/*",
    "packages/*"
  ],
  "engines": {
    "node": "12.14.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@types/mocha": "7.0.2",
    "@types/mockdate": "2.0.0",
    "@types/node": "12.12.39",
    "@types/sinon": "9.0.0",
    "@types/sinon-express-mock": "1.3.8",
    "@types/supertest": "2.0.9",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "check-node-version": "4.0.3",
    "dotenv": "8.2.0",
    "eslint": "7.0.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-recommended": "4.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "4.2.1",
    "husky": "4.2.5",
    "lerna": "3.21.0",
    "mocha": "7.1.2",
    "mochawesome": "6.1.1",
    "mockdate": "2.0.5",
    "node-rsa": "1.0.8",
    "nodemon": "2.0.4",
    "nyc": "15.0.1",
    "prettier": "2.0.5",
    "should": "13.2.3",
    "sinon": "9.0.2",
    "sinon-express-mock": "2.2.1",
    "source-map-support": "0.5.19",
    "supertest": "4.0.2",
    "ts-loader": "7.0.4",
    "ts-mocha": "7.0.0",
    "ts-node": "8.10.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3",
    "unit.js": "2.1.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  },
  "scripts": {
    "build": "yarn run:concurrent build",
    "bundle": "yarn run:concurrent bundle",
    "clean": "git clean -fdX -e '!.env' -e '!helm/mds/charts/*' -e '!helm/mds/files/*' -e '!helm/util/charts/*' && yarn",
    "image": "yarn run:concurrent image",
    "run:concurrent": "lerna run --stream",
    "run:sequential": "lerna run --stream --concurrency 1",
    "test": "check-node-version --package --print && yarn test:eslint && yarn test:unit",
    "test:eslint": "yarn run:concurrent test:eslint",
    "test:unit": "yarn run:sequential test:unit",
    "values": "bin/mdsctl templateValues"
  },
  "dependencies": {
    "chai": "4.2.0"
  }
}
