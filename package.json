{
  "name": "root",
  "version": "0.0.20",
  "private": true,
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "scripts": {
    "build": "tsc --version && pnpm run:concurrent build",
    "bundle": "webpack --version && pnpm run:concurrent bundle",
    "clean": "git clean -fdX -e '!.env' -e '!helm/mds/charts' -e '!helm/mds/charts/**' -e '!helm/mds/files' -e '!helm/mds/files/**' && pnpm install",
    "image": "pnpm run:concurrent image",
    "image:development": "pnpm run:concurrent image:development",
    "image:production": "pnpm run:concurrent image:production",
    "images": "pnpm run:concurrent image",
    "preinstall": "npx check-node-version --package && npx only-allow pnpm",
    "postinstall": "husky install",
    "lint": "pnpm lint:audit && pnpm lint:eslint '**/*.ts' && pnpm lint:prettier '**/package.json'",
    "lint:audit": "pnpm-ci audit -i 1556,1561 --strict",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-path .eslintignore",
    "lint:prettier": "prettier --check",
    "push": "bin/build-helper pushImages",
    "run:concurrent": "pnpm recursive run --filter=!root",
    "run:sequential": "pnpm recursive run --workspace-concurrency=1 --filter=!root",
    "test": "pnpm run:sequential test",
    "values": "bin/build-helper templateValues"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@types/mocha": "8.2.0",
    "@types/mockdate": "2.0.0",
    "@types/node": "14.14.20",
    "@types/sinon": "9.0.10",
    "@types/sinon-express-mock": "1.3.9",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "dotenv": "8.2.0",
    "eslint": "7.17.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-config-recommended": "4.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-log": "1.2.7",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.2.1",
    "git-revision-webpack-plugin": "3.0.6",
    "husky": "5.0.9",
    "jest": "26.6.3",
    "lerna": "3.22.1",
    "lint-staged": "10.5.4",
    "mocha": "8.2.1",
    "mochawesome": "6.2.1",
    "mockdate": "3.0.2",
    "node-rsa": "1.1.1",
    "nodemon": "2.0.7",
    "nyc": "15.1.0",
    "pnpm-ci-tools": "1.0.0",
    "prettier": "2.2.1",
    "prettier-plugin-packagejson": "2.2.9",
    "should": "13.2.3",
    "sinon": "9.2.3",
    "sinon-express-mock": "2.2.1",
    "source-map-support": "0.5.19",
    "supertest": "6.0.1",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.14",
    "ts-mocha": "8.0.0",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typedoc": "0.20.14",
    "typedoc-plugin-lerna-packages": "0.3.1",
    "typescript": "4.1.3",
    "unit.js": "2.1.1",
    "webpack": "5.13.0",
    "webpack-cli": "4.3.1",
    "webpack-merge": "5.7.3",
    "wtfnode": "0.8.4"
  },
  "engines": {
    "node": "14.2.0"
  }
}
