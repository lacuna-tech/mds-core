{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@mds-core/*": ["packages/*/index.ts"],
      "packages/*": ["/dev/null"],
      "*": ["@types/*", "*"]
    },
    "target": "ES2019",
    "skipLibCheck": true,
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "preserveSymlinks": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false
  }
}
